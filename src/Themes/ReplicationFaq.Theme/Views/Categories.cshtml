<!DOCTYPE html>
<html lang="@Orchard.CultureName()">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@RenderTitleSegments(Site.SiteName, "before")</title>
  @*
    Our CSS our custom stylesheet
    You need to spectify a version of dependent resource it it has a version
    https://docs.orchardcore.net/en/dev/docs/reference/modules/Resources/#registering-a-resource-manifest
    Raw file is at ./src/scss/style.scss
  *@
  <style asp-name="replication-faq-theme" at="Head"></style>

  <resources type="Meta" />
  <resources type="HeadLink" />
  <resources type="Stylesheet" /> <!-- Placeholder for style at Head -->
  <resources type="HeadScript" />
</head>

<body dir="@Orchard.CultureDir()">
  <div class="main-container">
    <div class="banner-nav">
      <nav class="nav-transparent">
        <div class="nav-container">
          <div class="logo">
            <span><i class="life-ring"></i>Home</span>
          </div>
          <div class="nav-bar-mobile" onclick="toggle(this)">
            <span><i class="bars"></i></span>
          </div>
          <div class="nav-items">
            <div>Home</div>
            <div>Distributor</div>
            <div>Snapshot</div>
            <div>Transactional</div>
            <div>Peer to Peer</div>
            <div>Merge</div>
            <div>Sync Services</div>
            <div>Always On</div>
            <div>Contact</div>
          </div>
        </div>
      </nav>
    </div>
    <div class="main-content">
      <div class="section-sub-nav">
        <div class="row">
          <div class="col-2">
            <span class="breadcrumb"><a>Home</a></span>
            <span class="breadcrumb"><a>Distributor</a></span>
          </div>
          <div class="col-2">
            <div class="input-group">
              <input type="search" placeholder="Enter search term here" autocomplete="off" />
              <span><i class="search"></i></span>
            </div>
          </div>
        </div>
      </div>
      <div class="main-page">
        <div class="sidebar">
          <h3 class="sidebar-current-title">Distributor</h3>
          <div class="sidebar-title active">Distributor</div>
          <div class="sidebar-title">Snapshot Replication</div>
          <div class="sidebar-title">Transactional Replication</div>
          <div class="sidebar-title">Peer to Peer Replication</div>
          <div class="sidebar-title">Merge Replication</div>
          <div class="sidebar-title">Sync Services</div>
          <div class="sidebar-title">Always On</div>
          <div class="sidebar-title">Troubleshooting</div>
          <div class="sidebar-title">Contact</div>
        </div>
        <div class="section-page">
          <h1 class="title">Distributor</h1>
          <p class="sub-title"></p>
          <div class="list">
            <div class="link">
              <div>
                <h3><a href="">Configuring a Local Distributor</a></h3>
              </div>
              <div class="icon">
                <a href="#"><i class="chevron-right"></i></a>
              </div>
            </div>
            <div class="link">
              <div>
                <h3><a href="">Configuring a Remote Distributor</a></h3>
              </div>
              <div class="icon">
                <a href="#"><i class="chevron-right"></i></a>
              </div>
            </div>
            <div class="link">
              <div>
                <h3><a href="">Configuring a Publisher to use Local Distributor</a></h3>
              </div>
              <div class="icon">
                <a href="#"><i class="chevron-right"></i></a>
              </div>
            </div>
            <div class="link">
              <div>
                <h3><a href="">Configuring a Publisher to use Remote Distributor</a></h3>
              </div>
              <div class="icon">
                <a href="#"><i class="chevron-right"></i></a>
              </div>
            </div>
            <div class="link">
              <div>
                <h3><a href="">Configuring Always On AGs with a Distributor</a></h3>
              </div>
              <div class="icon">
                <a href="#"><i class="chevron-right"></i></a>
              </div>
            </div>
          </div>
          <div class="section-pagination">
            <div class="action-previous">
              <div>
                <span><i class="chevron-left"></i></span> First
              </div>
              <div>
                <span><i class="chevron-left"></i></span> Previous
              </div>
            </div>
            <div class="pagination">
              <span class="number-page active"> 1 </span>
              <span class="number-page"> 2 </span>
              <span class="number-page"> 3 </span>
              <span class="number-page">...</span>
              <span class="number-page"> 18 </span>
              <span class="number-page"> 19 </span>
              <span class="number-page"> 20 </span>
            </div>
            <div class="action-next">
              <div>
                Next <span><i class="chevron-right"></i></span>
              </div>
              <div>
                Last <span><i class="chevron-right"></i></span>
              </div>
            </div>

          </div>
        </div>
      </div>
    </div>

    <div class="footer">
      <div class="row">
        <div class="col-2">
          <p>Copyright 2017 Powered by Code Love</p>
        </div>
        <div class="col-2">
          <a href="http://www.facebook.com"><span><i class="facebook"></i></span></a>
          <a href="#"><span><i class="google"></i></span></a>
          <a href="#"><span><i class="vimeo"></i></span></a>
          <a href="#"><span><i class="instagram"></i></span></a>
          <a href="#"><span><i class="twitter"></i></span></a>
          <a href="#"><span><i class="youtube"></i></span></a>
        </div>
      </div>
    </div>
    <resources type="FootScript" />

  </div>
  <div class="nav-menu-mobile" id="nav-menu-mobile">
    <div class="list-nav-menu-mobile" id="list-nav-menu-mobile">
      <div class="row">
        <div class="col-1 logo-mobile">
          <span><i class="life-ring"></i>Home</span>
        </div>
        <div class="col-1">Article</div>
        <div class="col-1">
          <div class="sub-menu">
            <a>Scrollspy</a>
          </div>
          <div class="sub-menu">
            <a>Narrow</a>
          </div>
        </div>
        <div class="col-1">Faq</div>
        <div class="col-1">Contact</div>
        <div class="col-1">Components</div>
      </div>
      <div class="row">
        <div class="col-1 text--center btn-support">
          <a class="">Support</a>
        </div>
      </div>
      <div class="row">
        <div class="col-1 text--center">
          <a href="http://www.facebook.com"><span><i class="facebook"></i></span></a>
          <a href="#"><span><i class="google"></i></span></a>
          <a href="#"><span><i class="twitter"></i></span></a>
        </div>
      </div>
    </div>
  </div>
  <script>
    document.getElementById('nav-menu-mobile').addEventListener('click', function (event) {
      console.log("clicked");
      const ignoreClickOnMeElement = document.getElementById('list-nav-menu-mobile')
      var isClickInsideElement = ignoreClickOnMeElement.contains(event.target);
      if (!isClickInsideElement) {
        document.getElementById('nav-menu-mobile').className = 'nav-menu-mobile'
      }
    });
    function toggle() {
      const navMenu = document.getElementById('nav-menu-mobile')
      const className = navMenu.className
      navMenu.className = className == 'nav-menu-mobile' ? 'nav-menu-mobile show' : 'nav-menu-mobile'
    }
  </script>
</body>

</html>